    "version": "0.2.0",
    "configurations": [
        {
            "name": "Client Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/TestOrleans.Client/bin/Debug/netcoreapp3.1/TestOrleans.Client.dll",
            "args": [],
            "cwd": "${workspaceFolder}/TestOrleans.Client",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Silo Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/TestOrleans.Silo/bin/Debug/netcoreapp3.1/TestOrleans.Silo.dll",
            "args": [],
            "cwd": "${workspaceFolder}/TestOrleans.Silo",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}